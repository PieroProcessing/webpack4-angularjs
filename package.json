{
  "name": "webpack4-angularjs",
  "version": "1.0.0",
  "description": "angularjs webapp in webpack 4",
  "main": "app.js",
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack -p",
    "start": "webpack-dev-server",
    "extract": "angular-gettext-cli --files './src/**/*.+(js|html)' --exclude './src/assets/*' --dest './src/assets/l10n/extract.pot'",
    "compile": "angular-gettext-cli --compile --files 'src/assets/l10n/po/*.po' --dest 'src/assets/l10n/translations.js' --format javascript --module webpack4.angularjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PieroProcessing/webpack4-angularjs.git"
  },
  "keywords": [
    "webpack",
    "angularjs",
    "webapp",
    "import",
    "multitemplating"
  ],
  "author": "piero pappalardo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PieroProcessing/webpack4-angularjs/issues"
  },
  "homepage": "https://github.com/PieroProcessing/webpack4-angularjs#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "angular-gettext": "^2.4.1",
    "angular-gettext-cli": "^1.2.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "exports-loader": "^0.7.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.2.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.5",
    "webpack-core": "^0.6.9",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.10.1",
    "@uirouter/angularjs": "^1.0.22",
    "angular": "^1.7.8",
    "angular-animate": "^1.7.8",
    "angular-cookies": "^1.7.8",
    "angular-sanitize": "^1.7.8",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "oclazyload": "^1.1.0"
  }
}
